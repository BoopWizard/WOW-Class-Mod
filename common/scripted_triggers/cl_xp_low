cl_warrior_xp_low = { #this should be able to be used instead of the warrior trigger every time, will test
		has_trait = cl_warrior
		is_ai = no
		is_available = yes
		NOT = {
		has_trait_xp = {
						trait = cl_warrior
						track = fury
						value >= 30
					}
		has_trait_xp = {
						trait = cl_warrior
						track = arms
						value >= 30
					}			
		has_trait_xp = {
						trait = cl_warrior
						track = prot
						value >= 30
					}				
		}
		
}